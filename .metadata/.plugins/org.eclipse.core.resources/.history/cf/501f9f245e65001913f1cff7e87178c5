<script type="text/javascript">
	function sum1(num) {
		var sum = 0;
		for (var i = 1; i < num; i++) {
			sum += i;
		}
		console.log(sum);
	}
	sum1(10);
	var sum2 = function(sum) {
		var sum = 0;
		for (var i = 1; i <= sum; i++) {
			sum += i;
		}
		console.log(sum);
	}
	sum2(10);
</script>
